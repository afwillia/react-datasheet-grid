  "dependencies": {
    "@tanstack/react-virtual": "^3.0.0-beta.18",
    "classnames": "^2.3.1",
    "fast-deep-equal": "^3.1.3",
    "react-resize-detector": "^10.0.1",
    "throttle-debounce": "^3.0.1"
