{
  "name": "@sage-bionetworks/react-datasheet-grid",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sage-bionetworks/react-datasheet-grid.git"
  },
  "version": "4.11.5-sage.3",
  "description": "An Excel-like React component to create beautiful spreadsheets.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "src/style.css",
    "**/*.css"
  ],
  "scripts": {
    "build": "vite build",
    "start": "vite preview",
    "format": "prettier --write src/. && prettier --write example/src/.",
    "lint": "eslint src/.",
    "test": "vitest run"
  },
  "keywords": [
    "react",
    "reactjs",
    "spreadsheet",
    "grid",
    "datasheet",
    "excel",
    "airtable",
    "notion",
    "table"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/style.css": "./dist/style.css",
    "./style.css": "./dist/style.css"
  },
  "author": "Nicolas Keller (https://github.com/nick-keller)",
  "type": "module",
  "license": "MIT",
  "homepage": "https://react-datasheet-grid.netlify.app/",
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jsdom": "^16.2.13",
    "@types/react": "^18.0.8",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/ui": "^4.0.18",
    "concurrently": "^6.2.0",
    "cpx": "^1.5.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jsdom": "^16.6.0",
    "prettier": "^2.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-react": "^4.0.1",
    "vitest": "^4.0.18"
